{
  "name": "@ts-api-kit/compiler",
  "version": "0.1.7",
  "description": "Plugin TypeScript para gerar automaticamente arquivos OpenAPI.json a partir de rotas TypeScript",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "typescript",
    "openapi",
    "compiler",
    "plugin",
    "api",
    "documentation",
    "generator"
  ],
  "author": "devzolo <contact@devzolo.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/devzolo/ts-api-core.git",
    "directory": "packages/ts-api-compiler"
  },
  "homepage": "https://github.com/devzolo/ts-api-core#readme",
  "bugs": {
    "url": "https://github.com/devzolo/ts-api-core/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "ts-api-compiler": "./src/cli.ts"
  },
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./cli": {
      "default": "./src/cli.ts"
    }
  },
  "scripts": {
    "dev": "node --watch --experimental-transform-types --no-warnings src/index.ts",
    "build": "tsc",
    "test": "node --test --experimental-transform-types --no-warnings",
    "lint": "cd ../../ && biome check packages/compiler",
    "lint:fix": "cd ../../ && biome check packages/compiler --write",
    "format": "cd ../../ && biome format packages/compiler",
    "format:fix": "cd ../../ && biome format packages/compiler --write",
    "check": "tsgo --skipLibCheck",
    "generate-openapi": "node --experimental-transform-types --no-warnings src/generate-openapi.ts"
  },
  "peerDependencies": {
    "@kitajs/html": "^4.2.9"
  },
  "dependencies": {
    "@hono/node-server": "^1.11.3",
    "@scalar/hono-api-reference": "^0.9.16",
    "@standard-schema/spec": "^1.0.0",
    "@ts-api-kit/core": "workspace:*",
    "@valibot/to-json-schema": "^1.3.0",
    "chokidar": "^3.6.0",
    "commander": "^14.0.0",
    "fast-glob": "^3.3.2",
    "hono": "^4.9.8",
    "pathe": "^2.0.3",
    "ts-morph": "^27.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "typescript": "^5.6.2"
  }
}