{
  "name": "@ts-api-kit/core",
  "version": "0.1.8",
  "description": "A modern TypeScript framework for building APIs with file-based routing, OpenAPI generation, and Valibot schema validation. Perfect for creating type-safe REST APIs with automatic documentation.",
  "exports": {
    ".": "./src/index.ts",
    "./server": "./src/server.ts",
    "./openapi": "./src/openapi/index.ts",
    "./utils": "./src/utils/index.ts",
    "./node": "./src/node/index.ts"
  },
  "imports": {
    "@hono/node-server": "npm:@hono/node-server@^1.11.3",
    "@kitajs/html": "npm:@kitajs/html@^4.2.9",
    "@scalar/hono-api-reference": "npm:@scalar/hono-api-reference@^0.9.16",
    "@standard-schema/spec": "npm:@standard-schema/spec@^1.0.0",
    "@valibot/to-json-schema": "npm:@valibot/to-json-schema@^1.3.0",
    "fast-glob": "npm:fast-glob@^3.3.2",
    "hono": "npm:hono@^4.6.7",
    "pathe": "npm:pathe@^2.0.3"
  },
  "peerDependencies": {
    "valibot": "npm:valibot@^1.1.0"
  },
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "typescript",
    "api",
    "framework",
    "hono",
    "valibot",
    "file-based-routing",
    "openapi",
    "validation"
  ],
  "author": "devzolo <contact@devzolo.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ts-api-kit/ts-api-kit.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/ts-api-kit/ts-api-kit#readme",
  "bugs": {
    "url": "https://github.com/ts-api-kit/ts-api-kit/issues"
  },
  "runtimes": {
    "deno": "1.0.0",
    "node": "18.0.0",
    "browser": "1.0.0"
  },
  "compatibility": {
    "deno": "1.0.0",
    "node": "18.0.0",
    "browser": "1.0.0"
  }
}
