{
  "$schema": "https://jsr.io/schema/config-file.v1.json",
  "name": "@ts-api-kit/compiler",
  "version": "0.2.0",
  "exports": {
    ".": "./src/index.ts",
    "./cli": "./src/cli.ts"
  },
  "license": "MIT",
  "publish": {
    "include": [
      "LICENSE",
      "README.md",
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.test.ts",
      "src/**/*.spec.ts",
      "src/**/*_test.ts",
      "src/**/__tests__/**",
      "src/**/__test__/**"
    ]
  }
}
